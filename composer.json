{
  "name": "yehor/shop",
  "description": "autoloader",
  "type": "project",
  "license": "MIT",
  "minimum-stability": "stable",
  "require": {
    "ext-pdo": "*"
  },
  "autoload": {
    "psr-4": {
      "config\\": "src/config",
      "model\\": "src/model/",
      "repository\\": "src/repository",
      "util\\": "src/util"
    }
  },
  "require-dev": {
    "phpstan/phpstan": "^0.12.91",
    "squizlabs/php_codesniffer": "3.*"
  }
}
